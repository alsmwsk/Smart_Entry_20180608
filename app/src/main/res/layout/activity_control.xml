<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/LinearLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/HyundaiBlue"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="match_parent"
        android:layout_height="180px"
        android:background="@color/HyundaiBlue"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageButton
            android:id="@+id/imgBack"
            android:layout_width="30px"
            android:layout_height="54px"
            android:layout_gravity="center"
            android:layout_marginLeft="48px"
            android:background="#00000000"
            android:scaleType="centerInside"
            app:srcCompat="@drawable/navibar_back" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="284dp"
            android:layout_height="52dp"
            android:layout_gravity="center_vertical|center_horizontal|center"
            android:layout_weight="1"
            android:gravity="center"
            android:text="공조 설정"
            android:textColor="#FFFFFF"
            android:textSize="48px" />

        <ImageView
            android:id="@+id/imageView9"
            android:layout_width="30px"
            android:layout_height="wrap_content"
            android:layout_marginRight="48px"
            android:src="@color/HyundaiBlue" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/Background"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="432px"
            android:layout_marginBottom="25px"
            android:layout_marginLeft="48px"
            android:layout_marginRight="48px"
            android:layout_marginTop="48px"
            android:background="#0b131d"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="200px"
                android:layout_gravity="center"
                android:gravity="center"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="120px"
                    android:layout_weight="1"
                    android:gravity="left|center"
                    android:text="차량 내부 온도"
                    android:textColor="#FFFFFF"
                    android:textSize="48px" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="right|center"
                    android:text="22.0"
                    android:textColor="#FFFFFF"
                    android:textSize="90px" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="48px"
                    android:layout_marginRight="96px"
                    android:gravity="left|center"
                    android:text="℃"
                    android:textColor="#FFFFFF"
                    android:textSize="54px" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="800px"
                    android:layout_height="8px"
                    android:layout_gravity="center"
                    android:layout_marginLeft="48px"
                    android:adjustViewBounds="false"
                    android:background="@drawable/bar_slide_color"
                    android:paddingTop="20px" />

                <ImageView
                    android:layout_width="75px"
                    android:layout_height="75px"
                    android:layout_gravity="center"
                    android:layout_marginRight="48px"
                    android:background="@drawable/seekbar_button" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="330px"
                android:layout_gravity="center"
                android:gravity="center"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="120px"
                    android:layout_weight="1"
                    android:gravity="left|center"
                    android:text="차량 내부 온도"
                    android:textColor="#FFFFFF"
                    android:textSize="48px" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="right|center"
                    android:text="22.0"
                    android:textColor="#FFFFFF"
                    android:textSize="90px" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20px"
                    android:gravity="left|center"
                    android:text="℃"
                    android:textColor="#FFFFFF"
                    android:textSize="54px" />

            </LinearLayout>

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/bar_slide_color" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="432px"
            android:layout_marginBottom="48px"
            android:layout_marginLeft="48px"
            android:layout_marginRight="48px"
            android:layout_marginTop="25px"
            android:background="#0b131d"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="200px"
                android:layout_gravity="center"
                android:gravity="center"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="120px"
                    android:layout_weight="1"
                    android:gravity="left|center"
                    android:text="시동 유지 시간"
                    android:textColor="#FFFFFF"
                    android:textSize="48px" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="right|center"
                    android:text="5"
                    android:textColor="#FFFFFF"
                    android:textSize="90px" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="48px"
                    android:layout_marginRight="96px"
                    android:gravity="left|center"
                    android:text="분"
                    android:textColor="#FFFFFF"
                    android:textSize="54px" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="800px"
                    android:layout_height="8px"
                    android:layout_gravity="center"
                    android:layout_marginLeft="48px"
                    android:adjustViewBounds="false"
                    android:background="@drawable/bar_slide_white"
                    android:paddingTop="20px" />

                <ImageView
                    android:layout_width="75px"
                    android:layout_height="75px"
                    android:layout_gravity="center"
                    android:layout_marginRight="48px"
                    android:background="@drawable/seekbar_button" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="330px"
                android:layout_gravity="center"
                android:gravity="center"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="40px"
                    android:layout_weight="1"
                    android:gravity="left|center"
                    android:text="차량 내부 온도"
                    android:textColor="#FFFFFF"
                    android:textSize="48px" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="right|center"
                    android:text="22.0"
                    android:textColor="#FFFFFF"
                    android:textSize="90px" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20px"
                    android:gravity="left|center"
                    android:text="℃"
                    android:textColor="#FFFFFF"
                    android:textSize="54px" />

            </LinearLayout>

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/bar_slide_color" />

        </LinearLayout>

    </LinearLayout>
</LinearLayout>