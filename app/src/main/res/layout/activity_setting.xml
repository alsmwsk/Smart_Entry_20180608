<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/LinearLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/Background"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="match_parent"
        android:layout_height="180px"
        android:background="@color/HyundaiBlue"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageButton
            android:id="@+id/imgBack"
            android:layout_width="30px"
            android:layout_height="54px"
            android:layout_gravity="center"
            android:layout_marginLeft="48px"
            android:background="#00000000"
            android:scaleType="centerInside"
            app:srcCompat="@drawable/navibar_back" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="284dp"
            android:layout_height="52dp"
            android:layout_gravity="center_vertical|center_horizontal|center"
            android:layout_weight="1"
            android:gravity="center"
            android:text="설정"
            android:textColor="#FFFFFF"
            android:textSize="48px" />

        <ImageView
            android:id="@+id/imageView9"
            android:layout_width="30px"
            android:layout_height="wrap_content"
            android:layout_marginRight="48px"
            android:src="@color/HyundaiBlue" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="180px"
            android:onClick="btnSmartEntryRegisteration"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginBottom="48px"
                android:layout_marginLeft="48px"
                android:layout_marginTop="48px"
                android:layout_weight="1"
                android:text="Smart Entry 등록"
                android:textColor="#FFFFFF"
                android:textSize="48px" />

            <ImageView
                android:id="@+id/imageView"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_gravity="center"
                android:layout_marginRight="42px"
                android:scaleType="fitCenter"
                android:src="@drawable/arrow_gray_right" />


        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="3px"
            android:layout_centerInParent="true"
            android:layout_gravity="center"
            android:background="#0B131D" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="180px"
            android:onClick="btnSmartEntryUserManagement"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginBottom="48px"
                android:layout_marginLeft="48px"
                android:layout_marginTop="48px"
                android:layout_weight="1"
                android:text="Smart Entry 사용자 관리"
                android:textColor="#FFFFFF"
                android:textSize="48px" />

            <ImageView
                android:id="@+id/imageView"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_gravity="center"
                android:layout_marginRight="42px"
                android:scaleType="fitCenter"
                android:src="@drawable/arrow_gray_right" />


        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="3px"
            android:layout_centerInParent="true"
            android:layout_gravity="center"
            android:background="#0B131D" />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="180px"
            android:onClick="btnLogout"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginBottom="48px"
                android:layout_marginLeft="48px"
                android:layout_marginTop="48px"
                android:layout_weight="1"
                android:text="로그아웃"
                android:textColor="#FFFFFF"
                android:textSize="48px" />

            <ImageView
                android:id="@+id/imageView"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_gravity="center"
                android:layout_marginRight="42px"
                android:scaleType="fitCenter"
                android:src="@drawable/arrow_gray_right" />


        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="3px"
            android:layout_centerInParent="true"
            android:layout_gravity="center"
            android:background="#0B131D" />
    </LinearLayout>
</LinearLayout>